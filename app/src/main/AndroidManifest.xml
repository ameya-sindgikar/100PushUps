<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ameya.sevenweekshundredpushupschallenge" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".IntroActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Week"
            android:label="@string/title_activity_week" >
        </activity>
        <activity
            android:name=".Day"
            android:label="@string/title_activity_day" >
        </activity>
        <activity
            android:name=".DaysList"
            android:label="@string/title_activity_days_list" >
        </activity>
        <activity
            android:name=".FinalTest"
            android:label="@string/title_activity_final_test" >
        </activity>
    </application>

</manifest>
